    <changeSet author="rajan.vora (generated)" id="1642688631443-10">
        <createView fullDefinition="true" viewName="RIMS_SECRIDL_SEARCHDETAIL_VIEW">CREATE OR REPLACE FORCE VIEW "RIMS_SECRIDL_SEARCHDETAIL_VIEW" ("REPORTING_ACTIVITY", "SERIAL_NUMBER", "REVISION_NUMBER", "LOW_SERIAL_NUMBER", "MATERIAL_LEVEL_CODE", "ITEM_NUMBER", "CONDITION_TEXT", "RESOLUTION_TEXT", "NSN_FSC", "NSN_NIIN", "FSC_MATERIAL_DESCRIPTION", "DRAWING_NUMBER", "RIDL_ID", "RIDL_TYPE", "METRIC1", "METRIC2", "INSPECTION_ATTRIBUTE", "INSPECTION_ATTRIBUTE_LITERAL", "PREENGINEERED_IND", "CATEGORY") AS SELECT
   rims_mir.reporting_activity,
   rims_mir.serial_number,
   rims_mir.revision_number,
   rims_mir.low_serial_number,
   rims_mir.material_level_code,
   rims_ridl.item_number,
   rims_sec_ridl.condition_text,
   rims_sec_ridl.resolution_text,
   rims_mir.nsn_fsc,
   rims_mir.nsn_niin,
   rims_mir.fsc_material_description,
   rims_mir.drawing_number,
   rims_ridl.ridl_id,
   rims_ridl.ridl_type,
   rims_ridl.metric1,
   rims_ridl.metric2,
   rims_ridl.inspection_attribute,
   mir_inspection_attribute.literal inspection_attribute_literal,
   rims_ridl.preengineered_ind,
   rims_ridl.category
FROM rims_mir, rims_ridl, rims_sec_ridl, mir_inspection_attribute
WHERE rims_mir.reporting_activity = rims_ridl.reporting_activity
AND rims_mir.serial_number = rims_ridl.serial_number
AND rims_mir.revision_number = rims_ridl.revision_number
AND rims_mir.material_level_code IN (4, 5, 7, 9, 10)
AND rims_ridl.reporting_activity = rims_sec_ridl.reporting_activity
AND rims_ridl.serial_number = rims_sec_ridl.serial_number
AND rims_ridl.revision_number = rims_sec_ridl.revision_number
AND rims_ridl.item_number = rims_sec_ridl.item_number
AND rims_ridl.inspection_attribute = mir_inspection_attribute.code(+)</createView>
    </changeSet>
